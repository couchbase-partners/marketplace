required:
  - runtimeConfigName
  - nameSuffix
  - clusterSize

properties:
  nameSuffix:
    type: string
  runtimeConfigName:
    type: string
  timeout:
    type: integer
    minimum: 300
    maximum: 3000
    default: 900
  clusterSize:
    type: integer

outputs:
  successPath:
    type: string
  failurePath:
    type: string