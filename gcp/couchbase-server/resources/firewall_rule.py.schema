required:
  - nameSuffix

properties:
  nameSuffix:
    type: string
  network:
    type: array
    default: [default]
    minItems: 1
    maxItems: 8
    x-googleProperty:
      type: GCE_NETWORK
      gceNetwork:
        allowSharedVpcs: True
        machineTypeProperty: serverNodeType
  accessCIDR:
    type: string
    default: 0.0.0.0/0

outputs:
  ruleTag:
    type: string